{
    "manifest_version": 3,
    "name": "Search & Locate Bookmarks",
    "description": "Extension for searching and locating your bookmarks.",
    "permissions": ["bookmarks"],
    "version": "1.0",
    "icons": {
        "16": "/icons/16.png",
        "32": "/icons/32.png",
        "128": "/icons/128.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "/icons/16.png",
            "32": "/icons/32.png",
            "128": "/icons/128.png"
        }
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "windows": "Alt+Q"
            }
        }
    }
}
